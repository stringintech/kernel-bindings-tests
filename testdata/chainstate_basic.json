{
  "name": "Chainstate Basic Operations",
  "tests": [
    {
      "id": "setup_regtest_with_blocks",
      "method": "chainstate.setup",
      "params": {
        "chain_type": "regtest",
        "blocks_hex": [
          "",
          "",
          ""
        ]
      },
      "expected": {
        "success": {
          "blocks_imported": 3,
          "tip_height": 3
        }
      }
    },
    {
      "id": "read_first_imported_block",
      "method": "chainstate.read_block",
      "params": {
        "height": 1
      },
      "expected": {
        "success": {
          "block_hex": "",
          "height": 1
        }
      }
    },
    {
      "id": "read_tip_block",
      "method": "chainstate.read_block",
      "params": {
        "tip": true
      },
      "expected": {
        "success": {
          "block_hex": "",
          "height": 3
        }
      }
    },
    {
      "id": "teardown_chainstate",
      "method": "chainstate.teardown",
      "params": {},
      "expected": {
        "success": {
          "success": true
        }
      }
    }
  ]
}
